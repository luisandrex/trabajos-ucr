@page "/"
@namespace PRIME_UCR.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


@using PRIME_UCR.Components
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="TODO: Poner descripcion relevante del sitio" />
    <title>PRIME@UCR</title>
    <base href="~/" />
    <link rel="stylesheet" href="_content/Radzen.Blazor/css/default.css">
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="_content/MatBlazor/dist/matBlazor.css" rel="stylesheet" /> <!-- Diosvier href for MatBlazor-->
    <link href="css/site.css" rel="stylesheet" />
    <link href="css/Multimedia.css" rel="stylesheet" />
    <link href="css/DrugSelectorComponent.css" rel="stylesheet">
    <link href="css/PaginationDropdown.css" rel="stylesheet">
    <link href="css/PaginationDrugSelector.css" rel="stylesheet">
    <link href="css/Prescriptions.css" rel="stylesheet">
    <!---to add files, it is still on testing, it probably wont stay here that long-->
    <script src="_content/BlazorInputFile/inputfile.js"></script>

    <!-- Atenienses ++ chart style-->
    <link href="~/css/Dashboard.css" rel="stylesheet" />
    <link href="_content/Blazored.Modal/blazored-modal.css" rel="stylesheet" />
    <!-- For CSS Isolation (.NET 5.0) -->
    <link href="PRIME@UCR.styles.css" rel="stylesheet" />
</head>

<body>
    <component type="typeof(App)" render-mode="Server" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_content/Fluxor.Blazor.Web/scripts/index.js"></script>
    <script src="~/js/blazor-modal.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="_framework/blazor.server.js"></script>
    <script src="~/js/show.js"></script>
    <script src="~/js/real-time-camara.js"></script>
    <script src="~/js/recorderWorker.js"></script>
    <script src="~/js/recorder.js"></script>
    <script src="~/js/real-time-audio.js"></script>
    <script src="~/js/real-time-video.js"></script>

    <!--Atenienses++ scripts to include Charts.js.Blazor-->
    <!-- Reference the included moment.js javascript file. -->
    <script src="_content/ChartJs.Blazor/moment-with-locales.min.js" type="text/javascript" language="javascript"></script>

    <!-- Reference the included ChartJs javascript file. -->
    <script src="_content/ChartJs.Blazor/Chart.min.js" type="text/javascript" language="javascript"></script>

    <!-- This is the glue between the C# code and the ChartJs charts -->
    <script src="_content/ChartJs.Blazor/ChartJsBlazorInterop.js" type="text/javascript" language="javascript"></script>

    <!-- Some styling -->
    <link rel="stylesheet" href="_content/ChartJs.Blazor/ChartJSBlazor.css" />

    <!-- Diosvier scripts to include blazortable-->
    <script src="_content/BlazorTable/BlazorTable.min.js"></script>
    <!-- Diosvier scripts to include MatBlazor-->
    <script src="_content/MatBlazor/dist/matBlazor.js"></script>
    <script src="_content/Radzen.Blazor/Radzen.Blazor.js"></script>

    <!-- Atenienses++ scripts for graph creation-->
    <script src="https://cdn.amcharts.com/lib/4/core.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/charts.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/themes/frozen.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/themes/animated.js"></script>
    <script src="https://www.amcharts.com/lib/4/lang/es_ES.js"></script>
    <!--Graph-->
    <script src="https://cdn.amcharts.com/lib/4/maps.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/geodata/data/countries2.js"></script>
    <script src="~/js/Charts/IncidentsVsTransportTypeComponentJS.js"></script>
    <script src="~/js/Charts/IncidentsVsTimeChartComponentJS.js"></script>
    <script src="~/js/Charts/IncidentsVsOriginLocationComponentJS.js"></script>
    <script src="~/js/Charts/IncidentsVsDestinationLocationComponentJS.js"></script>
    <script src="~/js/Charts/AppointmentsVsMedicalCenterComponentJS.js"></script>
    <script src="~/js/Charts/AppointmentsVsPatientComponentJS.js"></script>
    <script src="~/js/Charts/PatientVsMedicalRecordsWeightComponentJS.js"></script>
    <script src="~/js/Charts/PatientVsMedicalRecordsHeightComponentJS.js"></script>
    <script src="~/js/Charts/PatientVsMedicalRecordsPressureComponentJS.js"></script>
    <!--Atenienses++ Modal-->
    <script src="_content/Blazored.Modal/blazored.modal.js"></script>

    <script src="~/js/save-file.js"></script>
</body>
</html>
